{
  "manifest_version": 2,

  "name": "react-chrome-extension",
  "description": " experiment",
  "version": "1.0",
  "browser_action": {
   "default_icon": "chro-ext-news-logo.png",
   "default_popup": "index.html"
  },
  "permissions": [
   "activeTab", "storage", "notifications", "cookies", "<all_urls>"

   ],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["bundle.js"]
    }
  ]
}